$(document).ready(function() {
  if(_userdata["user_level"] == 0) { 
    jQuery("li.row:nth-child(2), li.row:nth-child(3)").addClass("sCategoria"); 
   
                                   jQuery(".sCategoria").click(function() {
        new Noty({
            type: "error",
            layout: "center",
            text: "Seção reservada aos usuários HoldUser"
        }).show();
    }));
}
});
